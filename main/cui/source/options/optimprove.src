        Pos = MAP_APPFONT ( 111 , 145 ) ;
        Size = MAP_APPFONT ( 100 , 10 ) ;
    };
    PushButton PB_SHOWDATA
	{
        HelpID = "cui:PushButton:RID_SVXPAGE_IMPROVEMENT:PB_SHOWDATA";
